  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "es2015",
    "baseUrl": ".",
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "./app/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
